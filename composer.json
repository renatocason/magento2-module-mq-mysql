{
    "name": "room40coding/magento2-module-mq-mysql",
    "description": "Magento 2 Message Queue OS MySQL Broker Implementation by renatocason",
    "require": {
        "room40coding/magento2-module-mq": "~1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.5.13",
        "squizlabs/php_codesniffer": "3.3.1",
        "phpmd/phpmd": "@stable",
        "php-coveralls/php-coveralls": "~2.1.0"
    },
    "conflict": {
        "renatocason/magento2-module-mq-mysql": "*",
        "renatocason/magento2-module-mq": "*"
    },
    "scripts": {
        "test": "phpunit"
    },
    "type": "magento2-module",
    "license": [
        "OSL-3.0"
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Rcason\\MqMysql\\": ""
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        },
        {
            "type": "vcs",
            "url": "https://github.com/room40coding/magento2-module-mq"
        }
    ]
}
